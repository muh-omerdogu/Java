class Animal{
    public Animal(){
        System.out.println("The class is Animals");
    }
}
class Dog extends Animal{
    public Dog(){
        System.out.println("The class is Dogs");
    }
    
}
class Kangal extends Dog{
    public Kangal(){
        System.out.println("The dog is Sivas Kangal dog");
    } 

}
public class FirstValue {
    public static void main(String args []){
        Animal z=new Animal();
        Dog y=new Dog();
 
        Kangal x=new Kangal();
        
        
        
    }
    
    
}
