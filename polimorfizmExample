 *
 */

 class Asker{
	public Asker() {
		
		
	}
	public void selamVer() {
		
	}
}
class Er extends Asker{
	public void selamVer() {
		System.out.println("Her Er bir askerdir.");
	}
}
class Yuzbasi extends Asker{
	public void selamVer() {{
		System.out.println("Her Yüzbaşı bir askerdir.");
	}
	}
	
}
class Albay extends Asker{
	public void selamVer() {{
		System.out.println("Her Albay bir askerdir.");
	}
}
class Yarbay extends Asker{
	public Yarbay() {
		
	}
		public void selamVer() {
			System.out.println("Her Albay bir askerdir.");
		}
	}
}

	
