package Test_1;

/**
 * @author OMERDOGU
 *
 */
public class BicycleDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
        // Create two different 
        // Bicycle objects
        Bicycle bike1 = new Bicycle(); //yeni bir ram da nesne oluşturuldu 
        Bicycle bike2 = new Bicycle(); //yeni bir ram da nesne oluşturuldu 

        // Invoke methods on 
        // those objects
        bike1.changeCadence(50); //yeni değerleri atıyor. 
        bike1.speedUp(10);//yeni değerleri atıyor. 
        bike1.changeGear(2);//yeni değerleri atıyor. 
        bike1.printStates();//yeni değerleri atıyor. 

        bike2.changeCadence(50);
        bike2.speedUp(10);
        bike2.changeGear(2);
        bike2.changeCadence(40);
        bike2.speedUp(10);
        bike2.changeGear(3);
        bike2.printStates();
        
/*
 * cadence:50 speed:160 gear:2
cadence:40 speed:170 gear:3
 
 */
	}
//resource :oracle.com
}
